<script>

window.onload = function go(){
   var refresh = setInterval(fillfield,15000);
   fillfield();
}

function fillfield()
{
   getversion("--fw_version","SW Version","sw_ver");
   getchannel("--link","rf_channel","channel");
   getband("--rf_band","rf_band","band");
   getfield("--drv_info","link_uptime","l_uptime");
   getfield("--drv_info","core_uptime","c_uptime");

}



function getfield(cmdname,field,htmlfield)
{
  var xmlttp;
  var str;
  var url = new String("/cmd.sh?mocap&get&"+cmdname);

  if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
    xmlhttp = new XMLHttpRequest();
  }
  else { // code for IE6, IE5
    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
  }

  xmlhttp.open("GET", url, false);
  xmlhttp.setRequestHeader("If-Modified-Since", "Thu, 1 Jan 1970 00:00:00 GMT"); // ACTION_TEC
  xmlhttp.send();

         str = xmlhttp.responseText;



         var re = new RegExp (field+"\\s*:\\s*(\\w*.(?=\\w)\\w*.(?=\\w)\\w*)", "g");

         document.getElementById(htmlfield).innerHTML= re.exec(str)[1];


}

function getband(cmdname,field,htmlfield)
{
  var xmlttp;
  var str;
  var url = new String("/cmd.sh?mocap&get&"+cmdname);
  var bands = new Array("D Low","D High","D Extended","E","F","C4","H");

console.error(url);

  if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
    xmlhttp = new XMLHttpRequest();
  }
  else { // code for IE6, IE5
    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
  }

  xmlhttp.open("GET", url, false);
  xmlhttp.setRequestHeader("If-Modified-Since", "Thu, 1 Jan 1970 00:00:00 GMT"); // ACTION_TEC
  xmlhttp.send();

         str = xmlhttp.responseText;

         var re = new RegExp (field+"\\s*:\\s*([0-9]*)", "g");

         document.getElementById(htmlfield).innerHTML="Band " + bands[re.exec(str)[1]];


}



function getversion(cmdname,field,htmlfield)
{
  var xmlttp;
  var str;
  var url = new String("/cmd.sh?mocap&get&--fw_version");

  if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
    xmlhttp = new XMLHttpRequest();
  }
  else { // code for IE6, IE5
    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
  }

  

  xmlhttp.open("GET", url, false);
  xmlhttp.send();

  str = xmlhttp.responseText;

  var re1 = new RegExp ("version_moca\\s*:\\s*(\\w*)", "g");
  var re2 = new RegExp ("version_major\\s*:\\s*(\\w*)", "g");
  var re3 = new RegExp ("version_minor\\s*:\\s*(\\w*)", "g");
  var re4 = new RegExp ("version_patch\\s*:\\s*(\\w*)", "g");

  document.getElementById(htmlfield).innerHTML=re1.exec(str)[1]+"."+re2.exec(str)[1]+"."+re3.exec(str)[1]+"."+re4.exec(str)[1]+".6200.7";


}


function getchannel(cmdname,field,htmlfield)
{
  var xmlttp;
  var str;
  var url = new String("/cmd.sh?mocap&get&"+cmdname);

  if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
    xmlhttp = new XMLHttpRequest();
  }
  else { // code for IE6, IE5
    xmlhttp = new ActiveXObject("Microsoft.XMLHTTP");
  }

  xmlhttp.open("GET", url, false);
  xmlhttp.setRequestHeader("If-Modified-Since", "Thu, 1 Jan 1970 00:00:00 GMT"); // ACTION_TEC
  xmlhttp.send();

         str = xmlhttp.responseText;

         var re = new RegExp (field+"\\s*:\\s*(\\w*.(?=\\w)\\w*.(?=\\w)\\w*)", "g");

         document.getElementById(htmlfield).innerHTML= (re.exec(str)[1]*25);


}







</script>
